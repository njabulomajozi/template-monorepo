{
  "name": "@repo-ai",
  "version": "0.0.1",
  "private": true,
  "description": "AI/ML utilities and model provider integrations",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "pnpm lint:eslint && pnpm lint:typescript",
    "lint:eslint": "eslint .",
    "lint:typescript": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "pnpm clean:build && pnpm clean:cache && pnpm clean:deps",
    "clean:build": "rm -rf dist",
    "clean:cache": "rm -rf .turbo",
    "clean:deps": "rm -rf node_modules"
  },
  "dependencies": {
    "@repo-shared": "workspace:^"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
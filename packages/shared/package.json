{
  "name": "@repo-shared",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "Shared types, utilities, and constants for the assistance system",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.mjs",
      "require": "./dist/constants.js"
    },
    "./middlewares": {
      "types": "./dist/middlewares.d.ts",
      "import": "./dist/middlewares.mjs",
      "require": "./dist/middlewares.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "pnpm lint:eslint && pnpm lint:typescript",
    "lint:eslint": "eslint .",
    "lint:typescript": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "pnpm clean:build && pnpm clean:cache && pnpm clean:deps",
    "clean:build": "rm -rf dist",
    "clean:cache": "rm -rf .turbo",
    "clean:deps": "rm -rf node_modules"
  },
  "dependencies": {
    
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.0.14",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.17.0",
    "zod": "^3.23.8"
  }
}